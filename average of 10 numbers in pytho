l1=[]
num=10
for n in range(num):
    numbers = float(input('Enter number '))
    l1.append(numbers) 
avg = sum(l1)/len(l1)
print(" ")
import time
time.sleep(1)
print("The average is ", round(avg,2))
#my code for the average of 10 numbers
